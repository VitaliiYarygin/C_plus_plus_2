#include <iostream>
#include <string>
using namespace std;

class Person {
public:
    string name;
    int age;
    string gender;
};

void print(Person* person) {
    cout << person->name << " is " << person->age << " years old and identifies as " << person->gender << endl;
}

int main() {
    Person person;
    person.name = "Harry";
    person.age = 23;
    person.gender = "male";

    cout << "Meet " << person.name << endl;
    print(&person);

    return 0;
}
